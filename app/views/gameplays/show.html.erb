<%- model_class = Gameplay -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:postion_x) %>:</strong></dt>
  <dd><%= @gameplay.postion_x %></dd>
  <dt><strong><%= model_class.human_attribute_name(:position_y) %>:</strong></dt>
  <dd><%= @gameplay.position_y %></dd>
  <dt><strong><%= model_class.human_attribute_name(:game_id) %>:</strong></dt>
  <dd><%= @gameplay.game_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @gameplay.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:chance_number) %>:</strong></dt>
  <dd><%= @gameplay.chance_number %></dd>
  <dt><strong><%= model_class.human_attribute_name(:is_hit) %>:</strong></dt>
  <dd><%= @gameplay.is_hit %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              gameplays_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_gameplay_path(@gameplay), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              gameplay_path(@gameplay),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
