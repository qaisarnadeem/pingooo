<%- model_class = PrizeRedemption -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @prize_redemption.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:game_id) %>:</strong></dt>
  <dd><%= @prize_redemption.game_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:country_specific_prize_id) %>:</strong></dt>
  <dd><%= @prize_redemption.country_specific_prize_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:status) %>:</strong></dt>
  <dd><%= @prize_redemption.status %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              prize_redemptions_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_prize_redemption_path(@prize_redemption), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              prize_redemption_path(@prize_redemption),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
